{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "canopy": {
      "root": "projects/canopy",
      "sourceRoot": "projects/canopy/src",
      "projectType": "library",
      "prefix": "lg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/canopy/tsconfig.lib.json",
            "project": "projects/canopy/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/canopy/src/test.ts",
            "tsConfig": "projects/canopy/tsconfig.spec.json",
            "karmaConfig": "projects/canopy/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/canopy/tsconfig.lib.json",
              "projects/canopy/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "canopy-docs": {
      "root": "projects/canopy-docs/",
      "sourceRoot": "projects/canopy-docs/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/canopy-docs",
            "index": "projects/canopy-docs/src/index.html",
            "main": "projects/canopy-docs/src/main.ts",
            "polyfills": "projects/canopy-docs/src/polyfills.ts",
            "tsConfig": "projects/canopy-docs/tsconfig.app.json",
            "assets": [
              "projects/canopy-docs/src/favicon.ico",
              "projects/canopy-docs/src/assets"
            ],
            "styles": ["projects/canopy-docs/src/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/canopy-docs/src/environments/environment.ts",
                  "with": "projects/canopy-docs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "canopy-docs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "canopy-docs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "canopy-docs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/canopy-docs/src/test.ts",
            "polyfills": "projects/canopy-docs/src/polyfills.ts",
            "tsConfig": "projects/canopy-docs/tsconfig.spec.json",
            "karmaConfig": "projects/canopy-docs/karma.conf.js",
            "styles": ["projects/canopy-docs/src/styles.scss"],
            "scripts": [],
            "assets": [
              "projects/canopy-docs/src/favicon.ico",
              "projects/canopy-docs/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/canopy-docs/tsconfig.app.json",
              "projects/canopy-docs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "canopy-docs-e2e": {
      "root": "projects/canopy-docs-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/canopy-docs-e2e/protractor.conf.js",
            "devServerTarget": "canopy-docs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "canopy-docs:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/canopy-docs-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "canopy",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}
