{
  "name": "@canopy-collective/canopy",
  "version": "0.0.0",
  "url": "git://github.com/canopy-collective/canopy.git",
  "scripts": {
    "commit": "npx git-cz",
    "ng": "ng",
    "start": "npm run storybook",
    "start:test-app": "ng serve",
    "build": "npm run build:icons && ng build && npm run build-styles",
    "build:test-app": "npm run build && ng build canopy-test-app --prod --outputPath ./test-dist",
    "build:icons": "npm run optimise:svgs && npm run generate:icons && npm run generate:pictograms",
    "generate:icons": "svg-to-ts -s projects/canopy/src/assets/icons -o projects/canopy/src/lib/icon -f icons.interface -p lgIcon -t IconName -i Icon -d KEBAB && node svg-info.ts icon/icons.interface.ts",
    "generate:pictograms": "svg-to-ts -s projects/canopy/src/assets/pictograms -o projects/canopy/src/lib/pictograms -f pictograms.interface -p lgPictogram -t PictogramName -i Pictogram -d KEBAB && node svg-info.ts pictograms/pictograms.interface.ts",
    "optimise:svgs": "svgo projects/canopy/src/assets/ -r --config=svgo.json",
    "dsm-storybook:publish": "dsm-storybook publish",
    "dsm-storybook:preview": "dsm-storybook preview",
    "test": "ng test",
    "test:ci": "ng test canopy --watch=false --browsers=ChromeHeadless",
    "lint": "npm run tslint && npm run stylelint",
    "tslint": "ng lint",
    "e2e": "ng e2e",
    "storybook": "start-storybook",
    "stylelint": "stylelint \"projects/**/*.scss\"",
    "build-storybook": "build-storybook --quiet",
    "build-styles": "webpack --config webpack.global.config.js"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "autoprefixer": "^8.6.5",
    "core-js": "^3.3.2",
    "date-fns": "^2.12.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "webpack": "^4.41.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular-devkit/build-ng-packagr": "^0.801.3",
    "@angular/cli": "^8.3.12",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@babel/core": "^7.6.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.3.5",
    "@invisionapp/dsm-storybook": "0.0.129",
    "@semantic-release/github": "^5.5.4",
    "@semantic-release/npm": "^5.2.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/angular": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "@stylelint/postcss-css-in-js": "^0.37.1",
    "@stylelint/postcss-markdown": "^0.36.1",
    "@types/jasmine": "^3.4.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.2",
    "babel-loader": "^8.0.6",
    "codelyzer": "^6.0.0",
    "color-convert": "^2.0.1",
    "commitizen": "^4.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-vars-ponyfill": "^2.1.2",
    "cz-conventional-changelog": "^3.0.2",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.9",
    "jasmine-core": "^3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.8.0",
    "ng-mocks": "^9.0.0",
    "ng-packagr": "^5.7.0",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.32",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "protractor": "~5.4.0",
    "semantic-release": "^15.13.27",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "svg-to-ts": "^2.2.1",
    "svgo": "^1.3.2",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.4.1",
    "tsickle": "^0.37.0",
    "tslint": "^6.1.2",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-group-ordering": "^2.1.2",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.3.9",
    "webpack-fix-style-only-entries": "^0.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && git add .",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "projects/canopy/src/**/*.svg": [
      "npm run build:icons"
    ],
    "projects/canopy/src/**/*.{html, json, js}": [
      "prettier --write"
    ],
    "projects/canopy/src/**/*.{scss, css}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "projects/canopy/src/**/*.ts": [
      "prettier --write",
      "tslint -p tsconfig.json -c tslint.json --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
