{
  "name": "@canopy-collective/canopy",
  "version": "0.0.0",
  "url": "git://github.com/canopy-collective/canopy.git",
  "scripts": {
    "commit": "npx git-cz",
    "ng": "ng",
    "start": "npm run storybook",
    "build": "ng build && npm run build-styles",
    "test": "ng test",
    "lint": "npm run tslint && npm run stylelint",
    "tslint": "ng lint",
    "e2e": "ng e2e",
    "storybook": "start-storybook",
    "stylelint": "stylelint \"projects/**/*.scss\"",
    "build-storybook": "build-storybook --quiet",
    "build-styles": "webpack --config ./webpack.scss.config.js",
    "pretty-quick": "pretty-quick"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "autoprefixer": "^8.6.5",
    "classnames": "^2.2.6",
    "core-js": "^3.3.2",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "webpack": "^4.41.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.12",
    "@angular-devkit/build-ng-packagr": "^0.801.3",
    "@angular/cli": "^8.3.12",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@babel/core": "^7.6.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@semantic-release/github": "^5.5.4",
    "@semantic-release/npm": "^5.2.0",
    "@storybook/addon-a11y": "^5.2.4",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-backgrounds": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-notes": "^5.2.4",
    "@storybook/addon-storysource": "^5.2.4",
    "@storybook/addon-viewport": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/angular": "^5.2.4",
    "@types/classnames": "^2.2.9",
    "@types/jasmine": "^3.4.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.2",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.1.2",
    "color-convert": "^2.0.1",
    "commitizen": "^4.0.3",
    "css-vars-ponyfill": "^2.1.2",
    "cz-conventional-changelog": "^3.0.2",
    "file-loader": "^4.2.0",
    "husky": "^3.0.9",
    "jasmine-core": "^3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "ng-packagr": "^5.7.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "protractor": "~5.4.0",
    "semantic-release": "^15.13.27",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.4.1",
    "tsickle": "^0.37.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.3.9",
    "webpack-fix-style-only-entries": "^0.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{html, json, js}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{scss, css}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.ts": [
      "prettier --write",
      "tslint -p tsconfig.json -c tslint.json --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
